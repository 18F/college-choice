{
  "name": "college-choice-js",
  "version": "0.0.0",
  "author": "18F",
  "license": "CC0",
  "description": "Scripts for the college-choice project.",
  "repository": {
    "type": "git",
    "url": "git@github.com:18F/college-choice.git"
  },
  "scripts": {
    "build": "browserify `js/src/browserify-args.sh`",
    "build:d3": "d3-bundler -- js/src/d3-bundle.js > js/src/d3.js",
    "watch": "watchify -v `js/src/browserify-args.sh`",
    "lint": "jshint -c .jshintrc js/src",
    "test-unit": "mocha -- test/unit",
    "test-unit-ci": "XUNIT_FILE=$CIRCLE_TEST_REPORTS/unit.xml mocha -R spec-xunit-file -- test/unit",
    "test-a11y": "./test/a11y.js",
    "test": "wdio test/wdio.conf.js",
    "test-quick": "wdio test/wdio.quick.js",
    "test-sauce": "wdio test/wdio.sauce.js",
    "test-ci": "./test/ci.sh"
  },
  "dependencies": {
    "aight": "^2.1.1",
    "async": "*",
    "d3": "^3.5.9",
    "document-register-element": "^0.5.0",
    "formdb": "^0.2.0",
    "leaflet": "^0.7.7",
    "tagalong": "^0.1.2"
  },
  "devDependencies": {
    "browserify": "^12.0.1",
    "d3-arrays": "0.0.4",
    "d3-bundler": "^0.2.9",
    "d3-color": "^0.2.4",
    "d3-dispatch": "^0.2.1",
    "d3-dsv": "^0.1.5",
    "d3-format": "^0.3.2",
    "d3-interpolate": "^0.1.1",
    "d3-scale": "^0.1.1",
    "d3-selection": "^0.4.8",
    "d3-time": "0.0.5",
    "d3-time-format": "^0.1.1",
    "d3-xhr": "^0.1.1",
    "extend": "^3.0.0",
    "http-server": "^0.8.5",
    "jshint": "^2.8.0",
    "minifyify": "^7.1.0",
    "mocha": "*",
    "pa11y": "^3.2.1",
    "request": "^2.67.0",
    "simple-statistics": "^1.0.1",
    "sinon": "^1.17.2",
    "spec-xunit-file": "0.0.1-3",
    "stream-array": "^1.1.1",
    "tito": "^0.4.0",
    "uglify-js": "^2.6.1",
    "watchify": "^3.6.1",
    "webdriverio": "^3.3.0",
    "xmlhttprequest": "^1.8.0"
  },
  "engines": {
    "node": ">= 4.2.2"
  }
}
