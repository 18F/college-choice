    "@babel/preset-env": "^7.3.1",
    "@mdi/font": "^4.4.95",
    "babelify": "^10.0.0",
    "browserify": "^12.0.1",
    "css-loader": "^3.2.0",
    "deepmerge": "^4.0.0",
    "extend": "^3.0.2",
    "fibers": "^3.1.1",
    "file-loader": "^4.2.0",
    "jshint": "^2.9.7",
    "mini-css-extract-plugin": "^0.8.0",
