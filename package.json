  },
  "scripts": {
    "serve": "jekyll serve --baseurl='' -w",
    "build": "webpack --devtool source-map --config webpack.picc.config.js && webpack --devtool source-map --config webpack.vue.config.js",
    "new-build": "webpack --devtool source-map --config webpack.picc.config.js && webpack --devtool source-map --config webpack.vue.config.js",
    "watch": "webpack --devtool source-map --config webpack.picc.config.js --watch & webpack --devtool source-map --config webpack.vue.config.js --watch",
    "new-watch": "webpack --devtool source-map --config webpack.picc.config.js --watch & webpack --devtool source-map --config webpack.vue.config.js --watch",
    "lint": "jshint -c .jshintrc js/src",
    "test-unit": "mocha -- test/unit",
    "test-unit-ci": "XUNIT_FILE=test/reports/mocha/unit.xml mocha -R spec-xunit-file -- test/unit",
    "test-a11y": "./test/a11y.js",
    "test": "wdio test/wdio.conf.js",
    "test-quick": "wdio test/wdio.quick.js",
    "test-sauce": "wdio test/wdio.sauce.js",
    "test-ci": "wdio test/wdio.ci.js"
  },
